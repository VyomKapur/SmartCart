<%- include("header") %>
<div class="shopping-cart">
    <h1>*shopping cart here*</h1>
</div>
<div class="shopping-list">
    <!-- shopping list items enter here -->
    <% user.shoppingList.forEach(element =>{ %>
        <p><%= element.shoppingListItem %></p>
    <% }) %>
    <form action="/addItemToShoppingList" method="post">
        <input type="text" name="shoppingListItem" id="">
        <button type="submit">+</button>
    </form>
</div>
<%- include("footer") %>